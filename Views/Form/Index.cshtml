@model asd.Models.FormModel
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    <form asp-action="Index" asp-controller="Form">
        <label asp-for="FirstName">Your first name</label>
        <input asp-for="FirstName" placeholder="Your name goes here" /><br>
        <label asp-for="LastName">Your Last name</label><br>
        <input asp-for="LastName" placeholder="Your Last name goes here" /><br>
        <label asp-for="Email">Your Email</label><br>
        <input asp-for="Email" placeholder="Your Email goes here" /><br>
        <input type="submit" />
    </form>
</body>
</html>
